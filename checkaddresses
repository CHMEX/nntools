#!/bin/bash
source coinlist
source config

function validateaddress ()
{
    ISMINE=$($komodocli -ac_name=$1 validateaddress $2 | jq .ismine)
    echo [$1] $ISMINE
}

for coins in "${coinlist[@]}"; do
    coin=($coins)
    if [[ ! ${ignoreacs[*]} =~ ${coin[0]} ]]; then
        validateaddress ${coin[0]} $nn_address
    fi
done
